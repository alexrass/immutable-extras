{
  "plugins": [
    "jest",
  ],
  "extends": [
    "eslint:recommended",
    "plugin:jest/recommended",
    "./test/setup/lint.js"
  ],
  "parserOptions": {
    "sourceType": "module"
  },
  "env": {
    "amd": true,
    "es6": true,
    "browser": true
  },
  "globals": {
    "module": false,
    "__dirname": false,
    "process": false,
    "Immutable": false,
    "global": false
  },
  "rules": {
    "array-bracket-spacing": [ 1, "always" ],
    "block-scoped-var": 1,
    "brace-style": 1,
    "camelcase": 1,
    "comma-dangle": [ 1, "always-multiline" ],
    "comma-spacing": 1,
    "comma-style": [ 1, "last" ],
    "curly": [ 1, "all" ],
    "dot-notation": [ 1, { "allowKeywords": true, "allowPattern": "^[a-z]+(_[a-z]+)+$" } ],
    "eqeqeq": [ 1, "allow-null" ],
    "guard-for-in": 1,
    "key-spacing": 1,
    "keyword-spacing": [ 1, { "after": true, "before": true } ],
    "new-cap": 0,
    "no-bitwise": 1,
    "no-caller": 1,
    "no-cond-assign": [ 1, "except-parens" ],
    "no-debugger": 1,
    "no-duplicate-imports": 1,
    "no-empty": 1,
    "no-eval": 1,
    "no-extra-parens": [ 1, "functions" ],
    "no-extra-semi": [ 1 ],
    "no-irregular-whitespace": 1,
    "no-iterator": 1,
    "no-loop-func": 1,
    "no-multi-str": 1,
    "no-new": 1,
    "no-plusplus": 0,
    "no-proto": 1,
    "no-script-url": 1,
    "no-sequences": 1,
    "no-shadow": 1,
    "no-undef": 2,
    "no-unused-vars": 1,
    "no-with": 1,
    "object-curly-spacing": [ 1, "always" ],
    "quotes": [ 1, "single" ],
    "semi": [ 1, "always" ],
    "strict": [ 1, "global"],
    "valid-typeof": 1,
    "wrap-iife": [ 1, "inside" ]
  }
}
